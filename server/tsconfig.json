{
	"compilerOptions": {
		"module": "commonjs",
		"esModuleInterop": true,
		"target": "esnext",
		"noImplicitAny": false,
		"moduleResolution": "node",
		"sourceMap": true,
		"incremental": true,
		"skipLibCheck": true,
		"outDir": "dist",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"strictNullChecks": true
	}
}